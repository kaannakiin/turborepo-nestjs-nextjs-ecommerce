  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/eslint": "9.6.1",
    "@types/node": "22.8.2",
    "@types/node-fetch": "^2.6.13",
    "eslint": "9.13.0",
    "prisma": "^6.9.0",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "argon2": "^0.44.0",
    "iconv-lite": "^0.7.0",
    "node-fetch": "^2.7.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts"
