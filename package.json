{
  "name": "draft-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "start:migrate": "turbo run db:deploy && turbo run start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^25.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "turbo": "^2.6.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "catalog": {
    "@mantine/core": "8.3.10",
    "@mantine/hooks": "8.3.10",
    "@mantine/dates": "8.3.10",
    "@mantine/dropzone": "8.3.10",
    "@mantine/notifications": "8.3.10",
    "@mantine/spotlight": "8.3.10",
    "@prisma/client": "^7.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "prisma": "^7.2.0",
    "pg": "^8.17.1",
    "@tabler/icons-react": "^3.34.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "swiper": "^12.0.3",
    "htmlparser2": "^10.0.0",
    "date-fns": "^4.1.0",
    "@paralleldrive/cuid2": "^3.0.6",
    "eslint": "^9.31.0",
    "typescript-eslint": "^8.37.0",
    "prettier": "^3.6.2",
    "argon2": "^0.44.0",
    "google-libphonenumber": "^3.2.43",
    "@types/google-libphonenumber": "^7.4.30",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.5",
    "@tailwindcss/postcss": "^4.1.5"
  }
}
